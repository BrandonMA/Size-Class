{
    "expo": {
        "entryPoint": "./App.tsx",
        "name": "Size-Class",
        "slug": "Size-Class",
        "version": "1.0.0",
        "orientation": "default",
        "icon": "./assets/icon.png",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true
        },
        "web": {
            "favicon": "./assets/favicon.png"
        },
        "android": {
            "softwareKeyboardLayoutMode": "pan"
        }
    }
}
